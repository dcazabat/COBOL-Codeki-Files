//DBRMLIB  DD  DSN=KC03AXX.CURSOS.DBRMLIB,DISP=SHR
//SYSTSPRT DD SYSOUT=*
//SYSTSIN  DD *
  DSN SYSTEM(DBDG)
  RUN  PROGRAM(DSNTIAD) PLAN(DSNTIA13) -
       LIB('DSND10.DBDG.RUNLIB.LOAD')
  BIND PLAN(CURSOXXX) MEMBER(PGMDB210) +
      CURRENTDATA(NO) ACT(REP) ISO(CS) ENCODING(EBCDIC)
  END
